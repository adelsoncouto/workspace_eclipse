org.eclipse.php.ui.editor.templates=<?xml version\="1.0" encoding\="UTF-8" standalone\="no"?><templates><template autoinsert\="true" context\="phpcomment" deleted\="false" description\="add the user as author" enabled\="true" id\="org.eclipse.php.ui.editor.templates.phpdoc.author" name\="author">@author Adelson Silva Couto\r\n</template><template autoinsert\="true" context\="php" deleted\="false" description\="Adelson classe" enabled\="true" name\="couclass">namespace Model\\${namespace};&\#13;\r\n&\#13;\r\ndate_default_timezone_set("America/Sao_Paulo");&\#13;\r\nif (strcmp(basename($$_SERVER["SCRIPT_NAME"]), basename(__FILE__)) \=\=\= 0) { exit("Acesso negado"); }&\#13;\r\n&\#13;\r\n/**&\#13;\r\n* Classe respons\u00E1vel por&\#13;\r\n* @author Adelson Silva Couto&\#13;\r\n*/&\#13;\r\nclass ${class_name} {&\#13;\r\n\t&\#13;\r\n\t/**&\#13;\r\n\t* Construtor da classe&\#13;\r\n\t*/&\#13;\r\n\tpublic function __construct(){&\#13;\r\n\t\t&\#13;\r\n\t}&\#13;\r\n\t&\#13;\r\n\t/**&\#13;\r\n\t* Destrutor da classe&\#13;\r\n\t*/&\#13;\r\n\tpublic function __destruct(){&\#13;\r\n\t\t&\#13;\r\n\t}&\#13;\r\n\t&\#13;\r\n\t/**&\#13;\r\n\t* M\u00E9todo respons\u00E1vel por retornar uma vers\u00E3o da classe em array&\#13;\r\n\t*/&\#13;\r\n\tpublic function toArray(){&\#13;\r\n\t\t$$attr \= get_object_vars($$this); &\#13;\r\n\t\tforeach($$attr as $$k\=&gt;$$v){&\#13;\r\n\t\t\tif(preg_match("/^_/",$$k)){&\#13;\r\n\t\t\t\tunset($$attr[$$k]);&\#13;\r\n\t\t\t}&\#13;\r\n\t\t}&\#13;\r\n\t\treturn $$attr;&\#13;\r\n\t}&\#13;\r\n}&\#13;\r\n</template><template autoinsert\="true" context\="php" deleted\="false" description\="Gera o m\u00E9todo toArray" enabled\="true" name\="coutoarray">/**&\#13;\r\n* M\u00E9todo respons\u00E1vel por retornar uma vers\u00E3o da classe em array&\#13;\r\n*/&\#13;\r\npublic function toArray(){&\#13;\r\n\t$$attr \= get_object_vars($$this); &\#13;\r\n\tforeach($$attr as $$k\=&gt;$$v){&\#13;\r\n\t\tif(preg_match("/^_/",$$k)){&\#13;\r\n\t\t\tunset($$attr[$$k]);&\#13;\r\n\t\t}&\#13;\r\n\t}&\#13;\r\n\treturn $$attr;&\#13;\r\n}</template><template autoinsert\="true" context\="php" deleted\="false" description\="Gera o get e o set" enabled\="true" name\="cougetandset">${cursor}&\#13;\r\n&\#13;\r\n/**&\#13;\r\n* @return ${tipo} $$${nomeVariavel}&\#13;\r\n*/&\#13;\r\npublic function get${nomeGetAndSet}(){&\#13;\r\n\treturn $$this-&gt;${nomeVariavel};&\#13;\r\n}&\#13;\r\n&\#13;\r\n/**&\#13;\r\n* @param ${tipo} $$a${nomeGetAndSet}&\#13;\r\n*/&\#13;\r\npublic function set${nomeGetAndSet}($$a${nomeGetAndSet}){&\#13;\r\n\t$$this-&gt;${nomeVariavel} \= $$a${nomeGetAndSet};&\#13;\r\n}</template><template autoinsert\="true" context\="php" deleted\="false" description\="Gera um array com resultado do banco do tipo ['nomeCampo'\=&gt;valor]" enabled\="true" name\="cousqllista">$$${lista} \= array();&\#13;\r\nwhile($$row \= $$${nomePrepareStatement}-&gt;fetch(\\PDO\:\:FETCH_ASSOC)){&\#13;\r\n\t$$r \= array();&\#13;\r\n\tforeach ($$row as $$k\=&gt;$$v){&\#13;\r\n\t\t$$r[Util\:\:underlineToMaiuscula($$k)] \= $$v;&\#13;\r\n\t}&\#13;\r\n\t$$${lista}[] \= $$r;&\#13;\r\n}&\#13;\r\nreturn $$${lista};</template><template autoinsert\="true" context\="php" deleted\="false" description\="Gera o construtor e destrutor da class" enabled\="true" name\="couconstruct_and_destruct">public function __constructor(${parametros}){&\#13;\r\n\t${cursor}&\#13;\r\n}&\#13;\r\n&\#13;\r\npublic function __destructor(){&\#13;\r\n}&\#13;\r\n</template><template autoinsert\="true" context\="php" deleted\="false" description\="Geral um consulta sql" enabled\="true" name\="couPrepareStatement">$$${NomePrepareStatement} \= SistemaConexao\:\:getInstance()-&gt;prepare(${sql});&\#13;\r\n$$${NomePrepareStatement}-&gt;execute(${parametros});&\#13;\r\n$$${NomeListaResult} \= array();&\#13;\r\nwhile($$_row \= $$${NomePrepareStatement}-&gt;fetch(\\PDO\:\:FETCH_ASSOC)){&\#13;\r\n  $$_r \= array();&\#13;\r\n  foreach ($$_row as $$k\=&gt;$$v){&\#13;\r\n    $$r_[Util\:\:underlineToMaiuscula($$k)] \= $$v;&\#13;\r\n  }&\#13;\r\n  $$${NomeListaResult}[] \= $$_r;&\#13;\r\n}&\#13;\r\nreturn $$${NomeListaResult};</template><template autoinsert\="true" context\="php" deleted\="false" description\="Cria uma vari\u00E1vel privada" enabled\="true" name\="couVpri">/** @var ${tipo} $$${variavel} */&\#13;\r\nprivate $$${variavel};&\#13;\r\n&\#13;\r\n${cursor}</template><template autoinsert\="true" context\="php" deleted\="false" description\="Catribuir um parametro para um field" enabled\="true" name\="couAttrToField">$$this-&gt;${variavel} \= $$a${Variavel};&\#13;\r\n${cursor}</template><template autoinsert\="true" context\="php" deleted\="false" description\="Converte para o $this-&gt;" enabled\="true" name\="this">$$this-&gt;${cursor}</template><template autoinsert\="true" context\="php" deleted\="false" description\="Cria uma vari\u00E1vel interna" enabled\="true" name\="couVariavel">$$${variavel} \= ${cursor};/** @var ${tipo} $$${variavel} */</template><template autoinsert\="true" context\="php" deleted\="false" description\="Cria uma fun\u00E7\u00E3o privada" enabled\="true" name\="couPriFunction">/**&\#13;\r\n* M\u00E9todo respons\u00E1vel por ${descricao}&\#13;\r\n*/&\#13;\r\nprivate function ${nome_funcao}(${parametro}){&\#13;\r\n\t${cursor}//TODO&\#13;\r\n}</template><template autoinsert\="true" context\="php" deleted\="false" description\="Cria uma fun\u00E7\u00E3o publica" enabled\="true" name\="couPubFunction">/**&\#13;\r\n* M\u00E9todo respons\u00E1vel por ${descri\u00E7\u00E3o}&\#13;\r\n*/&\#13;\r\npublic function ${nomeFuncao}(){&\#13;\r\n\t${cursor}//TODO&\#13;\r\n}</template></templates>
