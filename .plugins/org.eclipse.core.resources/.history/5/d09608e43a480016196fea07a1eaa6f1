<?php
namespace Model\Timesheet\Historico;


class DividiHora{
	
	/** @var array(\Model\Model\Horas) $_listHora */
	private $listHora = [];
	private $listHoraPorDiaAndTipo = [];
	
	
	/** @var \Model\Model\Horas $aListHora */
	public function __construct($aListHora){
		$this->listHora = $aListHora;
		$this->agruparHorasPorDia();
	}
	
	/**
	* Método responsável por agrupar as horas por dia e tipo
	*/
	private function agruparHorasPorDia(){
		
	}
	
	//////////GET AND SET
	
	
	
	/**
	* @return ["timesheet_dia_tipo"=[\Model\Model\Horas]] $listHoraPorDiaAndTipo
	*/
	public function getListHoraPorDiaAndTipo(){
		return $this->listHoraPorDiaAndTipo;
	}
	
	/**
	* @param ["timesheet_dia_tipo"=[\Model\Model\Horas]] $agetListHoraPorDiaAndTipo
	*/
	public function setListHoraPorDiaAndTipo($aListHoraPorDiaAndTipo){
		$this->listHoraPorDiaAndTipo = $aListHoraPorDiaAndTipo;
	}
	
	/**
	* @return array(\Model\Model\Horas) $listHora
	*/
	public function getGetListHora(){
